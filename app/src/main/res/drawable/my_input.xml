<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >

            <stroke android:width="1dip" android:color="#284661" />
            <gradient android:angle="-90" android:startColor="#e2e3e7" android:endColor="#d1d8ef"  />
        </shape>
    </item>

    <item >
        <shape android:shape="rectangle"  >

            <stroke android:width="1dip" android:color="#284661" />
            <gradient android:angle="-90" android:startColor="#e2e3e7" android:endColor="#bac7f2"  />
        </shape>
    </item>
</selector>